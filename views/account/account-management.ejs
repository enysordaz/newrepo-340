<h1><%= title %></h1>
<%- messages() %>

<h2 class="welcome">Welcome <%= accountData.account_firstname %></h2>
<!-- Update Account Info for ALL -->
<p class="update"><a href="/account/update/<%= accountData.account_id %>">Update Account Information</a></p>

<!-- Inventory Management Link ONLY for Employee/Admin -->
<% if (accountData.account_type === "Employee" || accountData.account_type === "Admin") { %>
  <hr>
    <h3>Inventory Management</h3>
    <p class="update"><a href="/inv/management">Go to Inventory Management</a></p>
<% } %>
